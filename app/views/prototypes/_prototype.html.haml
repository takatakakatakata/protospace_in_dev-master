.col-sm-4.col-md-3.proto-content.removelist
  .thumbnail
    = link_to image_tag(prototype.set_main_thumbnail), prototype_path(prototype)
    .caption
      %h3
        = prototype.title
      .proto-meta
        .proto-user
          = link_to prototype.user.name, user_path(prototype.user)
          .btn.btn-default.button{:id => "likes_buttons_#{prototype.id}"}
            = render partial: "likes/like",locals: {prototype:prototype}
        .proto-posted
          = prototype.posted_date
      %ul.proto-tag-list.list-inline
        - prototype.tags.each do |tag|
          %li
            %a.btn.btn-default{_pageexpand_: "#{tag[:id]}", href: "/tags/#{tag[:id]}"} #{tag[:tag]}
